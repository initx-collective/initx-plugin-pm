{
  "name": "@initx-plugin/pm",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.6.5",
  "description": "project manager plugin for initx",
  "author": "imba97",
  "license": "MIT",
  "homepage": "https://github.com/initx-collective/initx-plugin-pm#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:initx-collective/initx-plugin-pm.git"
  },
  "bugs": {
    "url": "https://github.com/initx-collective/initx-plugin-pm/issues"
  },
  "keywords": [
    "initx",
    "initx-plugin",
    "project",
    "manager"
  ],
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "release": "bumpp",
    "lint": "eslint --cache",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@initx-plugin/core": "^0.0.31",
    "@initx-plugin/utils": "^0.0.31",
    "pathe": "^2.0.3"
  },
  "devDependencies": {
    "@imba97/eslint-config": "^0.0.6",
    "@types/node": "^22.13.13",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "lint-staged": "^15.5.0",
    "simple-git-hooks": "^2.12.1",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --cache --fix"
  }
}
